% srtsinglesingle(Tab) sorts matrix Tab and deletes all redundant values


function Tab=sortsingle(Tab)
Tab=sort(Tab);
% excl=[];   %positions of the values to be excluded
% for i=2:length(Tab)
%     if Tab(i)==Tab(i-1), excl(end+1)=i; end
% end
% Tab(excl)=[];

if isempty(Tab), return; end

if isnumeric(Tab),
    same = doff(Tab);
else
    changes = ~strcmp(Tab, decale(Tab));
end
    
changes(1)=true;

Tab = Tab(changes);
